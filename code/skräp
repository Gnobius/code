def bike(year, model="mc", ):
    print(model, year)
    
    
def return_multiple():
    """ Returnerar 3 variabler av olika typer """
    a = "anka"
    b = 3
    c = ['Tre ankor i kostym']

    return a, b, c

def perform_addition(a=5, b=6):
    a, b = 5, 6

    return a+b

def check_type_variable(variable):
    print(type(variable))

def check_dir(variable):
    print(dir(variable))